{
  "version": 2,
  "builds": [
    { "src": "next.config.js", "use": "@vercel/next" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "strict-transport-security": "max-age=63072000; includeSubDomains; preload"
      },
      "continue": true
    },
    {
      "src": "https://fetrabeauty.com/(.*)",
      "dest": "https://www.fetrabeauty.com/$1",
      "status": 308
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "continue": true
    }
  ]
}

